local HWIDTable = loadstring(game:HttpGet("https://pastebin.com/raw/0tp8QssH"))()
local Whitelist = HWIDTable.WhitelistedIDs
local Blacklist = HWIDTable.BlacklistedIDs
local HWID = game:GetService("RbxAnalyticsService"):GetClientId()
local Player = game.Players.LocalPlayer

-- Made by $ melon#5256

--//-- Whitelist check
for Index, ID in pairs(Whitelist) do
    if ID ~= HWID then
        setclipboard(HWID)
        else
       print("HardwareID is whitelisted")
    end
 end